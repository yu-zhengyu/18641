<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhengyu.partb" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.MusicActivity"
            android:label="@string/title_activity_music" >
        </activity>
        <activity
            android:name=".ui.VideoActivity"
            android:label="@string/title_activity_video" >
        </activity>
        <activity
            android:name=".ui.ImageWallActivity"
            android:label="@string/title_activity_image_wall" >
        </activity>
        <activity
            android:name=".ui.EmailActivity"
            android:label="@string/title_activity_email" >
        </activity>
    </application>

</manifest>
